      "source": [
        "array = [\"水\", \"金\", \"地\", \"火\", \"木\", \"土\", \"天\", \"海\", \"冥\"]\n",
        "\n",
        "# 配列arrayの要素を1つずつ順番に出力する\n",
        "for taiyokei in array:\n",
        "    print(taiyokei)\n",
        "\n",
        "\n",
        "# カウンタ変数を用意する\n",
        "i = 0\n",
        "\n",
        "# 変数iの値が8になるまで、繰り返し処理（要素を1つずつ順番に出力）\n",
        "while i <= 8:\n",
        "    print(array[i])\n",
        "\n",
        "    # カウンタ変数の値を1増やす\n",
        "    i = i + 1"
      ]
